package bst;

public class BST {
private Node root;
public BST() {
	root=null;
}
public boolean insert(int data) {
	Node newNode=new Node(data);
	if(newNode==null) {
		return false;
	}
	if(root==null) {
		root=newNode;
		return true;
	}
	Node temp=root;
	if(data==temp.getData()) {
		return false;
	}
	if(data < temp.getData()) {
	if(temp.getLeft()==null)
	{
		temp=temp.getLeft();
		temp.setLeft(newNode);
		return true;
	}
	temp=temp.getLeft();
}
	else 
		if(temp.getRight()==null)
	{
		temp=temp.getRight();
		temp.setRight(newNode);
		return true;
	}
	temp=temp.getRight();
}
}

